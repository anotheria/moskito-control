{
        "@applications": [
            {
                "name": "AYN prod",
                "@components": [
                    {
                        "name": "web01.prod",
                        "category": "web",
                        "connectorType": "HTTP",
                        "location": "http://172.21.182.4:8080/"
                    },
                    {
                        "name": "web02.prod",
                        "category": "web",
                        "connectorType": "HTTP",
                        "location": "http://172.21.182.5:8080/"
                    },
                    {
                        "name": "biz01.prod",
                        "category": "biz",
                        "connectorType": "HTTP",
                        "location": "http://172.21.182.6:8280/rest/"
                    }
                ],
                "@charts": [
                    {
                        "name": "Sessions",
                        "@lines": [
                            {
                                "component": "web01.prod",
                                "accumulator": "SessionCount Cur Absolute",
                                "caption": "web01.prod"
                            },
                            {
                                "component": "web02.prod",
                                "accumulator": "SessionCount Cur Absolute",
                                "caption": "web02.prod"
                            }
                        ]
                    },
                    {
                        "name": "Payments, 5 min interval",
                        "@lines": [
                            {
                                "component": "web01.prod",
                                "accumulator": "PaymentCounter-All 5m",
                                "caption": "web01.prod"
                            },
                            {
                                "component": "web02.prod",
                                "accumulator": "PaymentCounter-All 5m",
                                "caption": "web02.prod"
                            }
                        ]
                    },
                    {
                        "name": "Payments, 1 hour interval",
                        "@lines": [
                            {
                                "component": "web01.prod",
                                "accumulator": "PaymentCounter-All 1h",
                                "caption": "web01.prod"
                            },
                            {
                                "component": "web02.prod",
                                "accumulator": "PaymentCounter-All 1h",
                                "caption": "web02.prod"
                            }
                        ]
                    },
                    {
                        "name": "ThreadCount",
                        "@lines": [
                            {
                                "component": "web01.prod",
                                "accumulator": "ThreadCount",
                                "caption": "web01.prod"
                            },
                            {
                                "component": "web02.prod",
                                "accumulator": "ThreadCount",
                                "caption": "web02.prod"
                            }
                        ]
                    }
                ]
            }
        ],
        "@connectors": [
            {
                "type": "HTTP",
                "className": "org.moskito.control.connectors.HttpConnector"
            }
        ]
    },

    "@statusUpdater": {
        "checkPeriodInSeconds": 10,
        "threadPoolSize": 10,
        "timeoutInSeconds": 60,
        "enabled": false

    },

    "@chartsUpdater": {
        "checkPeriodInSeconds": 10,
        "threadPoolSize": 10,
        "timeoutInSeconds": 60
    }
}